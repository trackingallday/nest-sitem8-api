{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/jest.setup.js"
  ],
  "globalSetup": "<rootDir>/setup.ts",
 
  "globalTeardown": "<rootDir>/teardown.ts",
  "globals": {
    "ts-jest": {
      "diagnostics": false
    }
  }
}
